  <div data-type="prop" data-prop="children">
    <div data-type="component" data-component="Side">
      <div data-type="prop" data-prop="children">
        <p>This will be displayed on the left hand-side</p>
      </div>
    </div>
    <div data-type="component" data-component="Side">
      <div data-type="prop" data-prop="children">
        <p>This will be displayed on the right hand-side</p>
      </div>
    </div>
  </div>
</div>